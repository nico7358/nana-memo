{
  "name": "nanamemo",
  "short_name": "nanamemo",
  "description": "A simple and cute memo app. Quickly capture your ideas, shopping lists, and tasks. Features include auto-save, pinning notes, search, and customization.",
  "start_url": "/",
  "scope": "/",
  "display": "standalone",
  "background_color": "#f1f5f9",
  "theme_color": "#ec4899",
  "icons": [
    {
      "src": "/icon-192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/icon-512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "application/octet-stream": [".mimibk", ".db"],
        "application/vnd.sqlite3": [".sqlite", ".sqlite3"]
      },
      "launch_type": "single-client"
    }
  ]
}
